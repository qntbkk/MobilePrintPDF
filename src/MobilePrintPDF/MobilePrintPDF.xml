<?xml version="1.0" encoding="utf-8" ?>
<widget id="MobilePrintPDF.widget.MobilePrintPDF" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>MobilePrintPDF</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
		<property key="targetClass" type="attribute" required="true">
            <caption>Base64 PDF</caption>
            <category>Behavior</category>
            <description>Assign Base 64 PDF</description>
            <attributeTypes>
                        <attributeType name="String"/>
            </attributeTypes>
        </property>
		<property key="buttonText" type="translatableString" required="false">
            <caption>Button Text</caption>
            <category>Appearance</category>
            <description>The text of the "print" button.</description>
        </property>
		<property key="fileNamePrefix" type="translatableString" required="false">
            <caption>Filename Prefix</caption>
            <category>Appearance</category>
            <description>The prefix of the filename. A timestamp will be appended.</description>
        </property>
    </properties>
</widget>
